extends ./layout
block content
	div(class='row wrapper')
	include ./footer
	script(type='text/javascript').
		var second = #{second};
		var url = '#{url}';
		var msg='#{msg}';
		var d=dialog({
		title:'#{title}',
		content:msg+',页面将在#{second}秒后跳转。'
		});
		d.showModal();
		setTimeout(function(){
		    window.location.href=url;
		},second*1000)